{
  "expo": {
    "name": "HealthUnity",
    "slug": "HealthUnity-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/HealthUnityIcon.jpg",
    "scheme": "healthunityfrontend",
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#2563eb",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.HealthUnityfrontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "backgroundColor": "#2563eb",
        "resizeMode": "contain",
        "mdpi": "./assets/images/splash.png",
        "hdpi": "./assets/images/splash.png",
        "xhdpi": "./assets/images/splash.png",
        "xxhdpi": "./assets/images/splash.png",
        "xxxhdpi": "./assets/images/splash.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.HealthUnityfrontend",
      "buildType": "apk"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "backgroundColor": "#2563eb",
          "resizeMode": "contain",
          "imageWidth": 200
        }
      ],
      "expo-web-browser",
      [
        "react-native-auth0",
        {
          "domain": "dev-kv01j6hec10g7jva.us.auth0.com"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3c70c028-f1db-400b-aecd-3106ffe6bacd"
      }
    },
    "owner": "wuubzi"
  }
}
